{
	"name": "Tactical URL Expander",
  	"version": "1.6",
  	"description": "This extension expands links from many url shortening services.",
  	"permissions": [
    	"tabs", "http://api.longurl.org/v2/*"
  	],
	"icons": { "48": "images/logo48.png",
			   "128": "images/logo128.png"
			 },
	"options_page" : "options.html",
  	"background_page" : "background.html",
  	"content_scripts": [
    {
    	"matches": ["http://*/*", "https://*/*"],
      	"js" : ["libs/jquery-1.5.min.js", "content.js"]
    }
  	],
	"minimum_chrome_version" : "9.0"
}