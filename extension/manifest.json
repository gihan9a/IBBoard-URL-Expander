{
	"name": "Short URL Expander/Cleaner",
	"version": "1.0.5",
	"description": "Know where you're going! Expand short URLs and clean/remove 'utm' tracking information. Based on Tactical URL Expander",
	"permissions": [
	"http://api.longurl.org/v2/*"
	],
	"icons": { "48": "images/logo48.png",
			   "128": "images/logo128.png"
			 },
	"options_page" : "options.html",
	"background_page" : "background.html",
	"content_scripts": [
	{
	"matches": ["http://*/*", "https://*/*"],
		"js" : ["libs/jquery-1.5.min.js", "content.js"]
	}
	],
	"minimum_chrome_version" : "9.0"
}